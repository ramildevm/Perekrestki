<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.motion.widget.MotionLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/motion_layout13_2"
    android:layout_width="380dp"
    android:layout_height="380dp"
    android:background="@drawable/scene13_1_back"
    app:layoutDescription="@xml/scene13_2_scene">

    <ImageView
        android:id="@+id/leftcar"
        android:layout_width="90dp"
        android:layout_height="54dp"
        android:src="@drawable/carb_left_to_left"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.598" />

    <ImageView
        android:id="@+id/rightcar"
        android:layout_width="90dp"
        android:layout_height="54dp"
        android:layout_marginEnd="4dp"
        android:rotation="180"
        android:src="@drawable/carp_left_to_right"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.428" />

    <ImageView
        android:id="@+id/bottomcar"
        android:layout_width="90dp"
        android:layout_height="54dp"
        android:layout_marginBottom="28dp"
        android:rotation="-90"
        android:src="@drawable/carr_left_to_left"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.593"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.motion.widget.MotionLayout>
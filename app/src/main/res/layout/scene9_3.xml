<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.motion.widget.MotionLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/motion_layout9_3"
    android:layout_width="380dp"
    android:layout_height="380dp"
    android:background="@drawable/scene9_2_back"
    app:layoutDescription="@xml/scene9_3_scene">

    <ImageView
        android:id="@+id/leftcar"
        android:layout_width="90dp"
        android:layout_height="54dp"
        android:layout_marginStart="68dp"
        android:rotation="42"
        android:src="@drawable/cary_left_to_left"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.57" />

    <ImageView
        android:id="@+id/bottomcar"
        android:layout_width="90dp"
        android:layout_height="54dp"
        android:rotation="-90"
        android:src="@drawable/carbl_left_to_right"
        android:translationX="60dp"
        android:translationY="-15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.motion.widget.MotionLayout>